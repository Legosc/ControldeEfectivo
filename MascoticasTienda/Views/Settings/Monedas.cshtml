@model IEnumerable<MascoticasTienda.Models.Moneda>

<p>
    @Html.ActionLink("Nueva Moneda", "CreateMoneda")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MonedaDes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MonedaDes)
        </td>
        <td>
            @Html.ActionLink("Editar", "ModificaMoneda", new { id=item.ID }) |
            @Html.ActionLink("Eliminar", "DeleteMoneda", new { id=item.ID })
        </td>
    </tr>
}

</table>
